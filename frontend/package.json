{
  "name": "ethereum-explorer-v2",
  "private": true,
  "version": "0.1.0",
  "description": "A lightweight ethereum block explorer",
  "repository": "https://github.com/T-Systems-RUS/explorer",
  "license": "GPL-3.0",
  "devDependencies": {
    "bower": "^1.3.1",
    "http-server": "^0.6.1",
    "jasmine-core": "^2.3.4",
    "karma": "~0.12",
    "karma-chrome-launcher": "^0.1.12",
    "karma-firefox-launcher": "^0.1.6",
    "karma-jasmine": "^0.3.5",
    "karma-junit-reporter": "^0.2.2",
    "protractor": "^2.1.0",
    "shelljs": "^0.2.6"
  },
  "scripts": {
    "postinstall": "node -e \"try { require('fs').symlinkSync(require('path').resolve('node_modules/@bower_components'), 'app/bower_components', 'junction') } catch (e) { }\"",
    "prestart": "yarn install",
    "start": "http-server ./app -a localhost -p 8000 -c-1",
    "pretest": "yarn install",
    "test": "karma start karma.conf.js",
    "test-single-run": "karma start karma.conf.js  --single-run",
    "preupdate-webdriver": "yarn install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor e2e-tests/protractor.conf.js",
    "update-index-async": "node -e \"require('shelljs/global'); sed('-i', /\\/\\/@@NG_LOADER_START@@[\\s\\S]*\\/\\/@@NG_LOADER_END@@/, '//@@NG_LOADER_START@@\\n' + sed(/sourceMappingURL=angular-loader.min.js.map/,'sourceMappingURL=bower_components/angular-loader/angular-loader.min.js.map','app/bower_components/angular-loader/angular-loader.min.js') + '\\n//@@NG_LOADER_END@@', 'app/index-async.html');\""
  },
  "dependencies": {
    "angular-chart.js": "^1.0.0-alpha8",
    "chart.js": "^2.1.6",
    "crypto-socket": "^1.0.2",
    "@bower_components/Chart.js": "nnnick/Chart.js#^2.1.6",
    "@bower_components/angular": "angular/bower-angular#>=1.3.0",
    "@bower_components/angular-bootstrap": "angular-ui/bootstrap-bower#~0.13.3",
    "@bower_components/angular-bootstrap-simple-chat": "irontec/angular-bootstrap-simple-chat#^0.4.0",
    "@bower_components/angular-loader": "angular/bower-angular-loader#~1.4.0",
    "@bower_components/angular-mocks": "angular/bower-angular-mocks#~1.4.0",
    "@bower_components/angular-route": "angular/bower-angular-route#~1.4.0",
    "@bower_components/angular-sanitize": "angular/bower-angular-sanitize#~1.4.0",
    "@bower_components/bignumber.js": "MikeMcl/bignumber.js#>=2.0.0",
    "@bower_components/bootstrap": "twbs/bootstrap#~3.3.1",
    "@bower_components/crypto-js": "brix/crypto-js#~3.1.4",
    "@bower_components/html5-boilerplate": "h5bp/html5-boilerplate#~5.2.0",
    "@bower_components/jquery": "jquery/jquery-dist#1.9.1 - 2",
    "@bower_components/lodash": "lodash/lodash#2.4.x",
    "@bower_components/web3": "ethereum/ethereum.js#~0.20.0"
  },
  "engines": {
    "yarn": ">= 1.0.0"
  }
}
